{
  "extends": "eslint:recommended",
  "env": {
    "browser": false,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "no-console": "off",
    // "strict": ["error", "global"],
    "curly": "warn",
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "jest/prefer-to-have-length": "warn"
    // "jest/valid-expect": "error"
  },
  "plugins": ["jest"]
}
