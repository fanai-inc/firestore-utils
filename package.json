{
  "name": "@fanai/firestore-utils",
  "version": "0.0.1",
  "description": "CLI for import/export of firestore database",
  "keywords": [
    "javascript",
    "nodejs",
    "cli",
    "firestore",
    "firebase"
  ],
  "homepage": "https://github.com/fanai-inc/firestore-utils/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/fanai-inc/firestore-utils/issues",
    "email": "oss@fanai.io"
  },
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "lint": "./node_modules/.bin/eslint src/**",
    "precommit": "lint-staged",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/*/*.js' '!(node_modules)/**/*.js'"
  },
  "files": [
    "index.js",
    "/src/**/*"
  ],
  "bin": {
    "firestore-utils": "./index.js"
  },
  "author": {
    "name": "Shaun Wallace"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fanai-inc/firestore-utils"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "await-to-js": "^2.0.1",
    "base64-stream": "^0.1.3",
    "chalk": "^2.4.1",
    "commander": "^2.16.0",
    "envinfo": "^5.10.0",
    "firebase-admin": "^5.12.1",
    "inquirer": "^6.0.0"
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "devDependencies": {
    "dotenv": "^6.0.0",
    "eslint": "^5.1.0",
    "eslint-plugin-jest": "^21.17.0",
    "husky": "^0.14.3",
    "jest": "^23.4.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "yarn format",
      "git add"
    ]
  }
}
