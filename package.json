{
  "name": "@fanai/firestore-utils",
  "version": "1.0.0-alpha.4",
  "description": "CLI for import/export of firestore database",
  "keywords": [
    "javascript",
    "nodejs",
    "cli",
    "firestore",
    "firebase",
    "cloud-firestore",
    "firestore-db",
    "firestore-export",
    "firestore-import"
  ],
  "homepage": "https://github.com/fanai-inc/firestore-utils/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/fanai-inc/firestore-utils/issues",
    "email": "oss@fanai.io"
  },
  "main": "./firestoreUtils.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src/**",
    "precommit": "lint-staged",
    "export": "node src/bin/index.js export",
    "import": "node src/bin/index.js import",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/*/*.js' '!(node_modules)/**/*.js'",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "bin": {
    "firestore-utils": "./src/bin/index.js"
  },
  "author": {
    "name": "Shaun Wallace"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fanai-inc/firestore-utils"
  },
  "license": "Apache-2.0",
  "preferGlobal": true,
  "dependencies": {
    "await-to-js": "^2.0.1",
    "base64-stream": "^0.1.3",
    "chalk": "^2.4.1",
    "commander": "^2.16.0",
    "envinfo": "^5.10.0",
    "firebase-admin": "^5.12.1",
    "inquirer": "^6.0.0",
    "minimatch": "^3.0.4",
    "snyk": "^1.161.1"
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "devDependencies": {
    "eslint": "^5.1.0",
    "eslint-plugin-jest": "^21.17.0",
    "husky": "^0.14.3",
    "jest": "^23.4.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "yarn format",
      "git add"
    ]
  },
  "snyk": true
}
